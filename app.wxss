/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
 .flex_row{
  display: flex;
  flex-direction: row;
}
.bottom_txt{
  color: #8C9AB1;
  font-size: 28rpx;
  text-decoration:underline;
}
page{
  height: 100%;
  width: 100%;
  margin: 0 0 0 0;
  padding: 0 0 0 0;
  border: none;
}
scroll-view{
  height: 100%;
  width: 100%;
  margin: 0 0 0 0;
  padding: 0 0 0 0;
  border: none;
}
